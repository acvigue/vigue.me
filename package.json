{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && yarn gsap_fix && yarn gsap_fix_dist",
    "fk_fix": "if which gsed > /tmp/test.txt; then gsed -i s,://,:xs, node_modules/@formkit/pro/index.mjs && gsed -i s,://,:xs, node_modules/@formkit/pro/index.cjs; else sed -i s,://,:xs, node_modules/@formkit/pro/index.mjs && sed -i s,://,:xs, node_modules/@formkit/pro/index.cjs; fi",
    "gsap_fix": "if which gsed > /tmp/test.txt; then gsed -i \"s|103,114,101,101,110,115,111,99,107,46,99,111,109|108,111,99,97,108,104,111,115,116|\" node_modules/gsap-trial/**.js; else sed -i \"s|103,114,101,101,110,115,111,99,107,46,99,111,109|108,111,99,97,108,104,111,115,116|\" node_modules/gsap-trial/**.js; fi",
    "gsap_fix_dist": "if which gsed > /tmp/test.txt; then gsed -i \"s|103,114,101,101,110,115,111,99,107,46,99,111,109|108,111,99,97,108,104,111,115,116|\" node_modules/gsap-trial/dist/**.js; else sed -i \"s|103,114,101,101,110,115,111,99,107,46,99,111,109|108,111,99,97,108,104,111,115,116|\" node_modules/gsap-trial/dist/**.js; fi"
  },
  "dependencies": {
    "nuxt": "^3.10.3",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "@heroicons/vue": "^2.1.1",
    "@tresjs/cientos": "^3.3.0-next.0",
    "@tresjs/core": "^3.7.0",
    "@tresjs/nuxt": "^2.1.1",
    "gsap-trial": "^3.12.5",
    "mitt": "^3.0.1",
    "three": "^0.162.0",
    "tweakpane": "^4.0.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@types/three": "^0.162.0",
    "sass": "^1.71.1",
    "vite-plugin-glsl": "^1.2.1"
  }
}
